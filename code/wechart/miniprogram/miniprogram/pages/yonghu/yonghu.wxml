<view class="list">
  <view class="item" hover-class="item_hover" wx:for="{{showList}}" wx:key="text" bindtap="go" data-index="{{index}}">
    {{item.text}}
    <image class="img" src="../images/qianwang.png"></image>
  </view>
</view>

<van-tabbar active="{{active}}" bind:change="onChange" active-color="#1296db" inactive-color="#000">
  <van-tabbar-item icon="../../pages/images/peizhi.png">基本信息</van-tabbar-item>
  <van-tabbar-item icon="../../pages/images/dingdan.png">进销存</van-tabbar-item>
  <van-tabbar-item icon="../../pages/images/paichan.png">财务管理</van-tabbar-item>
  <van-tabbar-item icon="../../pages/images/caozuo-huizong.png">用户管理</van-tabbar-item>
</van-tabbar>